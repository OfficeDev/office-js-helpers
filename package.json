{
  "name": "@microsoft/office-js-helpers",
  "description": "A collection of helpers to simplify development of Office Add-ins & Microsoft Teams Tabs",
  "version": "0.8.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficeDev/office-js-helpers.git"
  },
  "scripts": {
    "lint": "tslint -c ./tslint.json ./src/**/*.ts",
    "clean": "rimraf dist dts",
    "build:ts": "tsc -p tsconfig.json",
    "build:lib": "webpack --colors --progress --display-modules",
    "build:min": "uglifyjs ./dist/office.helpers.js --compress --mangle --output ./dist/office.helpers.min.js",
    "build": "npm run clean && npm run build:ts && npm run build:lib && npm run build:min",
    "version": "npm run build",
    "beta": "npm version prerelease && npm publish --tag beta"
  },
  "main": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/office.helpers.d.ts",
  "license": "MIT",
  "files": [
    "dist/",
    "dts/"
  ],
  "dependencies": {
    "core-js": "2.5.0",
    "crypto-js": "3.1.9-1",
    "lodash-es": "4.17.4",
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "4.14.6",
    "@types/microsoftteams": "1.0.0",
    "@types/node": "8.0.24",
    "@types/office-js": "^0.0.47",
    "@types/webpack": "3.0.9",
    "awesome-typescript-loader": "3.2.3",
    "babel-core": "6.26.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-preset-env": "1.6.0",
    "dts-bundle": "0.7.3",
    "html-loader": "0.5.1",
    "rimraf": "2.6.1",
    "tslint": "5.6.0",
    "typescript": "2.4.2",
    "uglify-js": "3.0.28",
    "webpack": "3.5.5",
    "webpack-closure-compiler": "2.1.5"
  },
  "keywords": [
    "Office Add-ins",
    "Office JavaScript API",
    "Authentication",
    "OAuth",
    "Storage",
    "Helpers"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/OfficeDev/office-js-helpers/issues"
  },
  "homepage": "https://github.com/OfficeDev/office-js-helpers#readme"
}